{
    "footer": {
        "blocks": [
            "footer-layout.desktop"
        ]
    },
    "footer-layout.desktop": {
        "children": [
            "flex-layout.row#footer-1-desktop"
        ]
    },
    "flex-layout.row#footer-1-desktop": {
        "children": [
            "flex-layout.col#footer-left-desktop",
            "flex-layout.col#footer-right-desktop"
        ],
        "props": {
            "blockClass": "footer-row",
            "paddingTop": 7,
            "paddingBottom": 7
        }
    },
    "flex-layout.col#footer-left-desktop": {
        "children": [
            "accepted-payment-methods"
        ]
    },
    "flex-layout.col#footer-right-desktop": {
        "children": [
            "social-networks"
        ]
    },
    "accepted-payment-methods": {
        "props": {
            "paymentMethods": [
                "mastercard",
                "visa",
                "diners club"
            ]
        }
    },
    "social-networks": {
        "props": {
            "showInColor": true,
            "socialNetworks": [
                {
                    "url": "https://instagram.com/foo",
                    "name": "Instagram"
                },
                {
                    "url": "https://facebook.com/foo",
                    "name": "Facebook"
                },
                {
                    "url": "https://twitter.com/foo",
                    "name": "Twitter"
                }
            ]
        }
    }
}